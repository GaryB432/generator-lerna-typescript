{
  "name": "generator-lerna-typescript",
  "version": "1.8.0",
  "description": "Get started with Lerna using TypeScript",
  "author": {
    "name": "Gary Bortosky",
    "email": "garybort@gmail.com"
  },
  "files": [
    "generators",
    "utils"
  ],
  "main": "generators/index.js",
  "keywords": [
    "lerna",
    "typescript",
    "monorepo",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^2.0.0",
    "yeoman-assert": "^3.1.1",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^24.9.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "case": "^1.6.2",
    "chalk": "^3.0.0",
    "yeoman-generator": "^4.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "GaryB432/generator-lerna-typescript",
  "license": "ISC"
}
